<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>br.com</groupId>
  <artifactId>WS_DFe</artifactId>
  <version>1.00</version>
  
  <build>
    <finalName>${project.artifactId}-${project.version}</finalName>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>mave-compiler-plugin</artifactId>
        <version>3.3</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-assembly-plugin</artifactId>
        <version>2.6</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>single</goal>
            </goals>
            <configuration>
              <descriptorRefs>
                <descriptorRef>jar-with-dependencies</descriptorRef>
              </descriptorRefs>
              <outputDirectory>${pasta.destino}</outputDirectory>
              <appendAssemblyId>false</appendAssemblyId>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
	      <groupId>org.apache.axis2</groupId>
	      <artifactId>axis2-wsdl2code-maven-plugin</artifactId>
	      <version>1.6.4</version>
	      <executions>
          <execution>
            <id>status2</id>
            <goals>
              <goal>wsdl2code</goal>
            </goals>
			      <configuration>
			        <outputDirectory>${sourceDirectory}</outputDirectory>
			        <wsdlFile>${basedir}/WSDL/NfeStatusServico2.wsdl</wsdlFile>
			        <overWrite>true</overWrite>
			      </configuration>
          </execution>
          <execution>
            <id>status</id>
            <goals>
              <goal>wsdl2code</goal>
            </goals>
            <configuration>
              <outputDirectory>${sourceDirectory}</outputDirectory>
              <wsdlFile>${basedir}/WSDL/NfeStatusServico.wsdl</wsdlFile>
              <overWrite>true</overWrite>
            </configuration>
          </execution>
          <execution>
            <id>consulta2</id>
            <goals>
              <goal>wsdl2code</goal>
            </goals>
            <configuration>
              <outputDirectory>${sourceDirectory}</outputDirectory>
              <wsdlFile>${basedir}/WSDL/NfeConsulta2.wsdl</wsdlFile>
              <overWrite>true</overWrite>
            </configuration>
          </execution>
          <execution>
            <id>consulta</id>
            <goals>
              <goal>wsdl2code</goal>
            </goals>
            <configuration>
              <outputDirectory>${sourceDirectory}</outputDirectory>
              <wsdlFile>${basedir}/WSDL/NfeConsulta.wsdl</wsdlFile>
              <overWrite>true</overWrite>
            </configuration>
          </execution>
          <execution>
            <id>inutilizacao2</id>
            <goals>
              <goal>wsdl2code</goal>
            </goals>
            <configuration>
              <outputDirectory>${sourceDirectory}</outputDirectory>
              <wsdlFile>${basedir}/WSDL/NfeInutilizacao2.wsdl</wsdlFile>
              <overWrite>true</overWrite>
            </configuration>
          </execution>
          <execution>
            <id>inutilizacao</id>
            <goals>
              <goal>wsdl2code</goal>
            </goals>
            <configuration>
              <outputDirectory>${sourceDirectory}</outputDirectory>
              <wsdlFile>${basedir}/WSDL/NfeInutilizacao.wsdl</wsdlFile>
              <overWrite>true</overWrite>
            </configuration>
          </execution>
          <execution>
            <id>retAutorizacao</id>
            <goals>
              <goal>wsdl2code</goal>
            </goals>
            <configuration>
              <outputDirectory>${sourceDirectory}</outputDirectory>
              <wsdlFile>${basedir}/WSDL/NfeRetAutorizacao.wsdl</wsdlFile>
              <overWrite>true</overWrite>
            </configuration>
          </execution>
          <execution>
            <id>evento</id>
            <goals>
              <goal>wsdl2code</goal>
            </goals>
            <configuration>
              <outputDirectory>${sourceDirectory}</outputDirectory>
              <wsdlFile>${basedir}/WSDL/recepcaoevento.wsdl</wsdlFile>
              <overWrite>true</overWrite>
            </configuration>
          </execution>
          <execution>
            <id>autorizacao</id>
            <goals>
              <goal>wsdl2code</goal>
            </goals>
            <configuration>
              <outputDirectory>${sourceDirectory}</outputDirectory>
              <wsdlFile>${basedir}/WSDL/NFeAutorizacao.wsdl</wsdlFile>
              <overWrite>true</overWrite>
            </configuration>
          </execution>
	      </executions>
	    </plugin>
    </plugins>
  </build>

  <properties>
    <pasta.destino>D:/Desenvolvimento/Java/LibCustom</pasta.destino>
    <versao.axis>1.6.4</versao.axis>
  </properties>
  
  <dependencies>
    <dependency>
      <groupId>org.apache.axis2</groupId>
      <artifactId>axis2-kernel</artifactId>
      <version>${versao.axis}</version>
    </dependency>
    <dependency>
      <groupId>org.apache.axis2</groupId>
      <artifactId>axis2-adb</artifactId>
      <version>${versao.axis}</version>
    </dependency>
    <dependency>
      <groupId>org.apache.axis2</groupId>
      <artifactId>axis2-transport-local</artifactId>
      <version>${versao.axis}</version>
    </dependency>
    <dependency>
      <groupId>org.apache.axis2</groupId>
      <artifactId>axis2-transport-http</artifactId>
      <version>${versao.axis}</version>
    </dependency>
    <!-- <dependency>
      <groupId>javax.mail</groupId>
      <artifactId>javax.mail-api</artifactId>
      <version>1.5.5</version>
    </dependency> -->
  </dependencies>
</project>